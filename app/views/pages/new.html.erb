
<!--隠して　Basic認証を使いページに行かせないよう　 -->
<!-- ブログ（文字、写真）を投稿可能にする-->


<%= form_with(model: @page, local: true, html: {multipart: true}, url: pages_path) do |form| %>

  
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :content %>
  <%= form.text_area :content %>

  <%= form.label :photo %>
  <%= form.file_field :image %>

  <!-- 他のフォームの要素... -->

  <%= form.submit 'Create Blog Post' %>
<% end %>















