<!-- ブログ（文字、写真）を投稿可能にする-->


<%= form_for @blog_post, html: { multipart: true } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :content %>
  <%= f.text_area :content %>

  <%= f.label :photo %>
  <%= f.file_field :photo %>

  <!-- 他のフォームの要素... -->

  <%= f.submit 'Create Blog Post' %>
<% end %>















